config_version=5

[application]

config/name="Saturn Bomberman"
run/main_scene="res://game/scenes/Shell.tscn"
run/low_processor_mode=true
boot_splash/show_image=false
config/icon="res://icon.png"
config/macos_native_icon="res://icon.png"
config/windows_native_icon="res://icon.png"
config/features=PackedStringArray("4.0", "C#")

[autoload]

Game="*res://game/scripts/Game.cs"
Preferences="*res://game/scripts/Preferences.cs"
NakamaNetwork="*res://game/scripts/network/NakamaNetwork.cs"
NetworkMessenger="*res://game/scripts/network/NetworkMessenger.cs"
Client="*res://game/scenes/Client.tscn"
Server="*res://server/Server.tscn"
LoadingScreen="*res://game/scenes/LoadingScreen.tscn"

[config]

description="A multiplayer implementation of the classical bomberman game.
One of the players should press 'host', while the other
should type in his address and press 'play'."
icon="res://players/assets/black_bomberman/head/head.png"
features=PackedStringArray("4.0")

[debug]

file_logging/enable_file_logging=true
gdscript/warnings/return_value_discarded=false

[display]

window/size/viewport_width=796
window/size/viewport_height=448
window/stretch/mode="viewport"
window/size/width=640
window/size/height=480
window/vsync/use_vsync=false

[filesystem]

import/blender/enabled=false
import/fbx/enabled=false

[flobb[application]

"config]name"="Multiplayer Bomber"

